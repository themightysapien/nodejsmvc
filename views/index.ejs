<% include includes/header %>
<div class="container">
  <div class="row">
    <div class="col-md-7">
      <h3>Users</h3>
      <table class="table">
        <thead>
          <tr>
            <th>SN</th>
            <th>Name</th>
            <th>Actions</th>
          </tr>
        </thead>
        <tbody>
        <%
        var i = 1;
        users.forEach(function(el, index){ %>
            <tr>
                <td><%= i %></td>
                <td> <%= el.email %> </td>
                <td> <%= el.email %> </td>
            </tr>
        <% i++; }); %>
        </tbody>
      </table>
    </div>
    <div class="col-md-5">
      <h3>Register</h3>
      <form action="#" method="post" class="form-horizontal">
        <div class="form-group">
          <label class="col-md-2">Email </label>
          <div class="col-md-8"><input name="email" class="form-control"></div>
        </div>
        <div class="form-group">
          <label class="col-md-2">Password </label>
          <div class="col-md-8"><input type="password" name="password" class="form-control"></div>
        </div>
        <div class="form-group">
          <label class="col-md-2"> </label>
          <div class="col-md-8"><input name="register" type="submit" class="btn btn-primary" value="Register"></div>
        </div>
      </form>
    </div>
  </div>
  
</div>
<script src="js/vendor/bootstrap.min.js"></script>

<script src="js/main.js"></script>
</body>
</html>
